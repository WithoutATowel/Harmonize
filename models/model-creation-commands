// USER MODEL
sequelize model:create --name user --attributes spotifyId:string,name:string,email:string,displayName:string,accessToken:string,refreshToken:string

// PLAYLIST MODEL
sequelize model:create --name playlist --attributes name:string,spotifyId:string

// ARTIST MODEL
sequelize model:create --name artist --attributes name:string,spotifyId:string,popularity:integer

// SONG MODEL
sequelize model:create --name song --attributes artistId:integer,name:string,spotifyId:string,popularity:integer,previewUrl:string

// USERS_PLAYLISTS MODEL
sequelize model:create --name users_playlists --attributes userId:integer,playlistId:integer

// USERS_ARTISTS MODEL
sequelize model:create --name users_artists --attributes userId:integer,artistId:integer

// USERS_SONGS MODEL
sequelize model:create --name users_songs --attributes userId:integer,songId:integer

// PLAYLISTS_SONGS MODEL
sequelize model:create --name playlists_songs --attributes playlistId:integer,songId:integer